{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "exclude": [
    "node_moudles",
    "plugins",
    "documentation",
    "pages",
    "dist"
],
"awesomeTypescriptLoaderOptions": {
  "forkChecker": true,
  "useWebpackText": true //Allows loaders to be chained to awesome-typescript-loader.
}
}
